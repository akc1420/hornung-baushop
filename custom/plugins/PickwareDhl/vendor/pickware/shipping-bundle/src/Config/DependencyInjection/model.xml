<?xml version="1.0" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <!-- Models -->
        <service id="Pickware\ShippingBundle\Config\Model\ShippingMethodConfigDefinition">
            <tag name="shopware.entity.definition" />
        </service>
        <!-- Extensions -->
        <service id="Pickware\ShippingBundle\Config\EntityExtension\CarrierExtension">
            <tag name="shopware.entity.extension" />
        </service>
        <service id="Pickware\ShippingBundle\Config\EntityExtension\ShippingMethodExtension">
            <tag name="shopware.entity.extension" />
        </service>
    </services>
</container>
